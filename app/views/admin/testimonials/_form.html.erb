<div class="container">
  <h1>Editing Testimonilas</h1>
  <%= tinymce_assets %>
  <%= form_for([:admin, @testimonial]) do |f| %>
    <%#= f.error_notification %>

      <div class="field">
        <%= f.label :title %></br>
        <%= f.text_field :title %>
      </div></br>

      <div class="field">
        <%= f.label :description %><br>
        <%= f.text_area :description,  :class => "tinymce", :rows => 10, :cols => 70 %>
      </div></br>

      <div class="field">
        <%= f.label :testimonial_image %><br>
        <%= f.file_field :testimonial_image, lavel: "Image" %>
      </div></br>    
      

    <div class="actions">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  <% end %>
  <%= tinymce %>
</div>