<%= form_for [:admin, @artist_profile], html: { multipart: true } do |f| %>
  
  <%= render partial: "admin/artist_profiles/profile_orders_section", locals: {f: f, po_index: increment_by_one(f.index)} %>  
    
  <div class="container" style="margin-top:30px">
    <div class="cust_detail_btn">
      <div class="text-center"> <%= f.submit "Save & Close", class: "send-btn" %> &nbsp; <%= button_to 'Cancel', artist_profiles_path, class: "reset-btn" %> </div>
    </div>
  </div>

  </br>

  
  
<% end %>

<script type="text/javascript">       
  $(function () {
    $('.datepick').each(function(){
      $(this).datetimepicker({
        sideBySide: true
      });
    });
  });
</script>
